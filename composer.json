{
    "name": "visualbuilder/filament-versionable",
    "description": "Effortlessly manage revisions of your Eloquent models in Filament with polymorphic user support (Fork of mansoor/filament-versionable).",
    "keywords": [
        "visualbuilder",
        "laravel",
        "filament",
        "filament-versionable",
        "polymorphic",
        "revisions"
    ],
    "homepage": "https://github.com/visualbuilder/filament-versionable",
    "support": {
        "issues": "https://github.com/visualbuilder/filament-versionable/issues",
        "source": "https://github.com/visualbuilder/filament-versionable"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mansoor Ahmed",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../versionable"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "visualbuilder/versionable": "*"
    },
    "require-dev": {
        "larastan/larastan": "3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.8.0",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Visualbuilder\\FilamentVersionable\\": "src/",
            "Visualbuilder\\FilamentVersionable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Visualbuilder\\FilamentVersionable\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Visualbuilder\\FilamentVersionable\\FilamentVersionableServiceProvider"
            ],
            "aliases": {
                "FilamentVersionable": "Visualbuilder\\FilamentVersionable\\Facades\\FilamentVersionable"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
